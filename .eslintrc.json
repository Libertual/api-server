{
    "extends": "airbnb-base",
    "rules": {
      "no-underscore-dangle": [2, { "allow": ["_id", "__v"] }],
      "comma-dangle": ["error", "never"],
      "curly": ["error", "multi", "consistent"],
      "no-multi-spaces": ["error",{
        "exceptions": {
           "VariableDeclarator": true
         }
      }],
      "no-console": ["warn", { "allow": ["warn", "error"] }]
    }
}
